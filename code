#include <iostream>
#include <cmath>
#include<fstream>
using namespace std;
int main() 
{
	int a,b;
	cout<<"輸入要算對數表的範圍 (以十為底) 取到小數點後15位"<<endl<<"輸入真數(起點)"<<endl; 
	cin>>a;
	cout<<"輸入真數(終點)"<<endl;
	cin>>b; 
	
	FILE * pFile;
    pFile = fopen ("LOG.txt","w");
    
	for(int i=a;i<=b;i++)
	{	
		printf("%.15f\n", log10(i));
		fprintf(pFile ,"log%d  =   ", i);
		fprintf(pFile ,"%.15f\n", log10(i));		
		
	}
	system("pause");
	return 0;
}
